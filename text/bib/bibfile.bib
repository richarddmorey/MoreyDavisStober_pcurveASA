@article{montoyaInconsistencyPcurveTesting2024,
  title = {The Inconsistency of P-Curve: {{Testing}} Its Reliability Using the Power Pose and {{HPA}} Debates},
  shorttitle = {The Inconsistency of P-Curve},
  author = {Montoya, R. Matthew and Kershaw, Christine and Jurgens, Christopher T.},
  year = {2024},
  month = jul,
  journal = {PLOS ONE},
  volume = {19},
  number = {7},
  pages = {e0305193},
  publisher = {Public Library of Science},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0305193},
  urldate = {2024-09-09},
  abstract = {Recent works have called into question whether p-curve can reliably assess the presence of "evidential value" within a set of studies. To examine an as-yet unexplored issue, we examined the method used to identify p-values for inclusion in a p-curve analysis. We developed iterated p-curve analysis (IPA), which calculates and p-curves every permutation for a set of reported p-values, and applied it to the data reported in several published p-curve analyses. Specifically, we investigated two phenomena for which p-curves have been used to evaluate the presence of evidential value: the power pose and the hypothalamic-pituitary-adrenal (HPA) reactivity debates. The iterated p-curve analyses revealed that the p-curve method fails to provide reliable estimates or reproducible conclusions. We conclude that p-curve should not be used to make conclusions regarding the presence or absence of evidence for a specific phenomenon.},
  langid = {english},
  keywords = {Binomials,Cortisol,Metaanalysis,Permutation,Psychology,Statistical distributions,Statistical methods,Testosterone},
  file = {/Users/saprm3/Zotero/storage/4QDMYW73/Montoya et al. - 2024 - The inconsistency of p-curve Testing its reliabil.pdf}
}

