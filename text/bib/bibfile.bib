  title = {If {{You Record It}}, {{Some Won}}'t {{Come}}: {{Using Lecture Capture}} in {{Introductory Psychology}}},
  shorttitle = {If {{You Record It}}, {{Some Won}}'t {{Come}}},
  author = {Drouin, Michelle A.},
  year = {2014},
