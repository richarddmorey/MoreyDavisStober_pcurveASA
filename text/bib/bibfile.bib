@article{cohenSecondOrderAsymptotic1982,
  title = {Second Order Asymptotic and Non-Asymptotic Optimality Properties of Combined Tests},
  author = {Cohen, Arthur and Marden, John I. and Singh, Kesar},
  year = {1982},
  month = jan,
  journal = {Journal of Statistical Planning and Inference},
  volume = {6},
  number = {3},
  pages = {253--276},
  issn = {0378-3758},
  doi = {10.1016/0378-3758(82)90030-1},
  urldate = {2024-03-14},
  abstract = {Let p independent test statistics be available to test a null hypothesis concerned with the same parameter. The p are assumed to be similar tests. Asymptotic and non-asymptotic optimality properties of combined tests are studied. The asymptotic study centers around two notions. The first is Bahadur efficiency. The second is based on a notion of second order comparisons. The non-asymptotic study is concerned with admissibility questions. Most of the popular combining methods are considered along with a method not studied in the past. Among the results are the following: Assume each of the p statistics has the same Bahadur slope. Then the combined test based on the sum of normal transforms, is asymptotically best among all tests studied, by virtue of second order considerations. Most of the popular combined tests are inadmissible for testing the noncentrality parameter of chi-square, t, and F distributions. For chi-square a combined test is offered which is admissible, asymptotically optimal (first order), asymptotically optimal (second order) among all tests studied, and for which critical values are obtainable in special cases. Extensions of the basic model are given.},
  keywords = {Admissibility,Bahadur Efficiency,Combined Tests,Fisher's Method,Gamma Transform,Normal Transform,Second Order Efficiency},
  file = {/Users/saprm3/Zotero/storage/BT3Y9A5M/Cohen et al. - 1982 - Second order asymptotic and non-asymptotic optimal.pdf;/Users/saprm3/Zotero/storage/EASSRP6V/0378375882900301.html}
