@article{legakiEffectChallengebasedGamification2020,
  title = {The Effect of Challenge-Based Gamification on Learning: {{An}} Experiment in the Context of Statistics Education},
  shorttitle = {The Effect of Challenge-Based Gamification on Learning},
  author = {Legaki, Nikoletta-Zampeta and Xi, Nannan and Hamari, Juho and Karpouzis, Kostas and Assimakopoulos, Vassilios},
  year = {2020},
