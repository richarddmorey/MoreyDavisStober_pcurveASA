@article{axelssonIdentificationAcutelySick2018,
  title = {Identification of Acutely Sick People and Facial Cues of Sickness},
  author = {Axelsson, John and Sundelin, Tina and Olsson, Mats J. and Sorjonen, Kimmo and Axelsson, Charlotte and Lasselin, Julie and Lekander, Mats},
  year = {2018},
  month = jan,
  journal = {Proceedings of the Royal Society B: Biological Sciences},
  volume = {285},
  number = {1870},
  pages = {20172430},
  publisher = {Royal Society},
  doi = {10.1098/rspb.2017.2430},
  urldate = {2022-02-11},
  abstract = {Detection and avoidance of sick individuals have been proposed as essential components in a behavioural defence against disease, limiting the risk of contamination. However, almost no knowledge exists on whether humans can detect sick individuals, and if so by what cues. Here, we demonstrate that untrained people can identify sick individuals above chance level by looking at facial photos taken 2 h after injection with a bacterial stimulus inducing an immune response (2.0 ng kg-1 lipopolysaccharide) or placebo, the global sensitivity index being d{$\prime$} = 0.405. Signal detection analysis (receiver operating characteristic curve area) showed an area of 0.62 (95\% confidence intervals 0.60--0.63). Acutely sick people were rated by naive observers as having paler lips and skin, a more swollen face, droopier corners of the mouth, more hanging eyelids, redder eyes, and less glossy and patchy skin, as well as appearing more tired. Our findings suggest that facial cues associated with the skin, mouth and eyes can aid in the detection of acutely sick and potentially contagious people.},
  keywords = {disease avoidance,facial cues,health,social perception},
  file = {/Users/saprm3/Zotero/storage/6HNM9PAN/Axelsson et al. - 2018 - Identification of acutely sick people and facial c.pdf}
}

